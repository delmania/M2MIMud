 import edu.rit.m2mi.Eoid;
 public interface PlayerCacheListener
 {
 	/**
	 * Used to report a player timeout
	 * @@param playerId The id of the player that timed out
	 */
	public void playerTimeout( Eoid playerId );
 }  
 
